{
    "classId": "idm.component.console.istickytree",
    "comName": "悬浮树",
    "className": "IStickyTree",
    "comType": "common",
    "comLangue": "vue",
    "compositeAttr": [
        {
            "type": "input",
            "layoutType": "inline",
            "text": "唯一标识",
            "bindKey": "ctrlId",
            "disabled": true,
            "default": "@[packageid]",
            "desc": "",
            "helpUrl": ""
        },
        {
            "type": "group",
            "text": "基本属性",
            "desc": "提供设置组件能达到展示效果的基本属性设置信息",
            "children": [
                {
                    "type": "radio",
                    "styleType": "radio",
                    "layoutType": "inline",
                    "text": "粘性布局",
                    "bindKey": "isOpenSticky",
                    "desc": "是否开启粘性布局，开启后在设计器内失效，可在预览页面时效果正常",
                    "default": true,
                    "dictionary": [
                        {
                            "label": "开启",
                            "value": true
                        },
                        {
                            "label": "关闭",
                            "value": false
                        }
                    ]
                },
                {
                    "type": "radio",
                    "styleType": "radio",
                    "layoutType": "inline",
                    "text": "搜索框",
                    "bindKey": "isShowSearch",
                    "desc": "是否显示搜索框",
                    "default": true,
                    "dictionary": [
                        {
                            "label": "显示",
                            "value": true
                        },
                        {
                            "label": "隐藏",
                            "value": false
                        }
                    ]
                },
                {
                    "type": "radio",
                    "styleType": "radio",
                    "layoutType": "inline",
                    "text": "连接线",
                    "bindKey": "isShowLine",
                    "desc": "是否显示连接线",
                    "default": false,
                    "dictionary": [
                        {
                            "label": "显示",
                            "value": true
                        },
                        {
                            "label": "隐藏",
                            "value": false
                        }
                    ]
                },
                {
                    "type": "input",
                    "layoutType": "inline",
                    "text": "字段替换",
                    "bindKey": "replaceFieldsStr",
                    "desc": "替换 treeNode 中 title,key,children 字段为 treeData 中对应的字段",
                    "default": "{ \"children\": \"children\", \"title\": \"title\", \"key\": \"key\" }"
                }
            ]
        },
        {
            "type": "group",
            "text": "样式设置",
            "desc": "点击？查看样式设置用法指南",
            "helpUrl": "",
            "children": [
                {
                    "type": "input",
                    "layoutType": "inline",
                    "text": "搜索框宽",
                    "desc": "搜索框宽度",
                    "bindKey": "searchWidth",
                    "default": ""
                },
                {
                    "type": "inputNumber",
                    "layoutType": "inline",
                    "text": "顶部吸顶",
                    "desc": "sticky布局top值,单位：px",
                    "bindKey": "stickyTop",
                    "default": 80
                },
                {
                    "type": "inputNumber",
                    "layoutType": "inline",
                    "text": "显示层级",
                    "desc": "z-index的值",
                    "bindKey": "zIndex",
                    "default": "10"
                },
                {
                    "type": "inlineGroup",
                    "text": "宽高",
                    "children": [
                        {
                            "type": "input",
                            "layoutType": "block",
                            "text": "宽",
                            "desc": "填写auto则为自适应，或者使用px、%、vw等单位，比如100%、100px、100vw等等",
                            "bindKey": "width",
                            "width": "60px",
                            "default": "100%"
                        },
                        {
                            "type": "input",
                            "layoutType": "block",
                            "text": "高",
                            "desc": "填写auto则为自适应，或者使用px、%、vh等单位，比如100%、100px、100vh等等",
                            "bindKey": "height",
                            "width": "60px",
                            "default": "auto"
                        }
                    ]
                },
                {
                    "type": "box",
                    "layoutType": "block",
                    "text": "内外边距",
                    "bindKey": "box"
                },
                {
                    "type": "colorPicker",
                    "layoutType": "inline",
                    "text": "背景色",
                    "bindKey": "bgColor",
                    "placeholder": "请选择颜色",
                    "default": {
                        "hex": "#FFFFFF",
                        "hex8": "#FFFFFFFF"
                    }
                },
                {
                    "type": "border",
                    "layoutType": "block",
                    "text": "边框",
                    "bindKey": "border"
                },
                {
                    "type": "font",
                    "layoutType": "block",
                    "text": "文字样式",
                    "bindKey": "font"
                },
                {
                    "type": "font",
                    "layoutType": "block",
                    "text": "匹配文字样式",
                    "bindKey": "matchFont",
                    "default": {
                        "fontColors": {
                            "hex": "#F50000",
                            "hex8": "#F50000FF"
                        }
                    }
                }
            ]
        },
        {
            "type": "group",
            "text": "高级",
            "desc": "点击？查看高级设置用法指南",
            "children": [
                {
                    "type": "input",
                    "layoutType": "inline",
                    "text": "表单标识",
                    "bindKey": "formFiledKey",
                    "desc": "如果为空则会提交表单时用唯一标识作为key",
                    "default": ""
                },
                {
                    "type": "dataSourceSelect",
                    "layoutType": "inline",
                    "text": "数据源",
                    "bindKey": "dataSource"
                },
                {
                    "type": "pageModuleSelect",
                    "layoutType": "inline",
                    "text": "联动组件",
                    "desc": "选择点击菜单需要联动的组件",
                    "bindKey": "triggerComponents",
                    "multiple": true
                },
                {
                    "type": "actionSelect",
                    "layoutType": "block",
                    "text": "自定义点击函数",
                    "bindKey": "customClickFunction",
                    "desc": "设置自定义点击函数后，默认点击事件取消，接受参数selectKeys",
                    "multiple": false
                }
            ]
        }
    ]
}
